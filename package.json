{
  "name": "yandex-shri-2018-homework-9",
  "version": "0.0.0",
  "description": "Домашнее задание #9 в ШРИ Яндекса 2018.",
  "scripts": {
    "build-js": "babel src/es6 -d dist/es6",
    "build-flow": "babel src/flow -d dist/flow",
    "build-ts": "tsc",
    "lint-js": "eslint src/es6/**/*.js",
    "lint-flow": "flow --include-warnings",
    "lint-ts": "tslint -c tslint.json -p tsconfig.json",
    "watch-js": "watch 'npm run test-js' src/es6 test/es6",
    "watch-flow": "watch 'npm run test-flow' src/flow test/flow",
    "watch-ts": "watch 'npm run test-ts' src/typescript test/typescript",
    "test-js": "mocha test/es6/**/*.test.js",
    "test-flow": "mocha --require babel-core/register test/flow/**/*.test.js",
    "test-ts": "mocha -r ts-node/register test/typescript/**/*.test.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ruslankhh/yandex-shri-2018-homework-9.git"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/mocha": "^5.0.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-minify": "^0.2.0",
    "babel-preset-stage-3": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "eslint": "^4.15.0",
    "eslint-config-semistandard": "^12.0.1",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "flow-bin": "^0.69.0",
    "mocha": "^5.0.5",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "tslint-config-semistandard": "^7.0.0",
    "typescript": "^2.8.1",
    "watch": "^1.0.2"
  },
  "license": "ISC"
}
